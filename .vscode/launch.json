    "version": "0.2.0",
    "configurations": [
        {
            "type": "by-gdb",
            "request": "launch",
            "name": "Debug ssh",
            "program": "ssh",
            "cwd": "${workspaceRoot}",
            "programArgs": "-v sftpuser@127.0.0.1"
        },
        {
            "type": "by-gdb",
            "request": "launch",
            "name": "Debug sshd",
            "program": "sshd",
            "cwd": "${workspaceRoot}",
            "programArgs": "-d"
        }
    ]
}